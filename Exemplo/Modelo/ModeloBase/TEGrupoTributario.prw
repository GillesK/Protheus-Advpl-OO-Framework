Method iniCampos() class TEGrupoTributario
	// Nome externo, nome interno, tipo
	local cpoDef
	cpoDef := {{"filial"					, "ZZN_FILIAL"	, "C"};
				,{"grupoTributario"		, "ZZN_GRTRIB"	, "C"};
				,{"aliquota"				, "ZZN_ALIQ"		, "N"};
				,{"ufOrigemRepasseIcms"	, "ZZN_UFREP"		, "N"}}
	::addCpoDef(cpoDef)	
		
	::setChave({{"1",{ "ZZN_FILIAL", "ZZN_GRTRIB"}}})
	
return

